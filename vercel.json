{
  "headers": [
    {
      "source": "/image/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Expires",
          "value": "@1y"
        }
      ]
    },
    {
      "source": "/icon/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Expires",
          "value": "@1y"
        }
      ]
    },
    {
      "source": "/model/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Expires",
          "value": "@1y"
        }
      ]
    },
    {
      "source": "/video/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        },
        {
          "key": "Expires",
          "value": "@30d"
        }
      ]
    },
    {
      "source": "/:path*\\.(jpg|jpeg|gif|png|webp|avif|svg|woff|woff2|ttf|eot|otf)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Expires",
          "value": "@1y"
        }
      ]
    },
    {
      "source": "/_next/image(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Expires",
          "value": "@1y"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "/:path*",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.(?<domain>.*)"
        }
      ],
      "destination": "https://:domain/:path*"
    }
  ]
}
